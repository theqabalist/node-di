{
  "name": "hub",
  "version": "0.0.0",
  "private": true,
  "author": "Brandon Keown <bkeown@blitz.io>",
  "description": "A simple helpful robot for your Company",
  "scripts": {
    "coverage": "istanbul cover --include-all-sources --root lib _mocha -- --reporter nyan 'spec/**/*.spec.js'",
    "hubot": "hubot --adapter slack",
    "lint": "eslint .",
    "pre-commit-msg": "echo 'Pre-commit linting and unit testing' && exit 0",
    "test": "mocha spec/**/*.spec.js --reporter min",
    "test-watch": "mocha --watch spec/**/*.spec.js --reporter min"
  },
  "dependencies": {
    "ascii-table": "^0.0.9",
    "aws-sdk": "^2.49.0",
    "bluebird": "^3.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coffeescript": "^1.12.5",
    "eslint": "^3.19.0",
    "hubot": "^2.19.0",
    "hubot-diagnostics": "0.0.1",
    "hubot-google-images": "^0.2.6",
    "hubot-google-translate": "^0.2.0",
    "hubot-help": "^0.2.0",
    "hubot-heroku-keepalive": "^1.0.2",
    "hubot-maps": "0.0.2",
    "hubot-pugme": "^0.1.0",
    "hubot-redis-brain": "0.0.3",
    "hubot-rules": "^0.1.1",
    "hubot-scripts": "^2.17.2",
    "hubot-shipit": "^0.2.0",
    "hubot-slack": "^4.3.4",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "pre-commit": "^1.2.2",
    "ramda": "^0.23.0",
    "sinon": "^2.2.0",
    "verror": "^1.10.0"
  },
  "pre-commit": [
    "pre-commit-msg",
    "lint",
    "test"
  ]
}
